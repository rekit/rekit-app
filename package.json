{
  "name": "rekit-app",
  "author": "Nate Wang",
  "version": "3.0.0",
  "private": true,
  "dependencies": {
    "electron-log": "^2.2.17",
    "electron-promise-ipc": "^0.1.3",
    "electron-store": "^2.0.0",
    "electron-util": "^0.10.2",
    "get-port": "^4.0.0",
    "lodash": "^4.17.5",
    "node": "^10.10.0",
    "rekit-core": "^2.0.0",
    "rekit-studio": "^3.0.0"
  },
  "main": "./node/main.js",
  "homepage": "./",
  "scripts": {
    "start": "node scripts/start.js",
    "start-electron": "cross-env NODE_ENV=development electron .",
    "start-electron-dist": "cross-env NODE_ENV=production electron ./",
    "build-static": "node ./scripts/buildStatic.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "compression": "normal",
    "appId": "com.webows.rekit",
    "productName": "Rekit",
    "copyright": "Copyright Â© 2018 ${author}",
    "asar": true,
    "asarUnpack": [
      "node_modules"
    ],
    "npmRebuild": false,
    "files": [
      "build",
      "node"
    ],
    "dmg": {
      "icon": "icon.icns"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "identity": "Pei Wang (ZUQ2GPKPP2)"
    }
  },
  "rekit": {
    "devPort": 6093,
    "studioPort": 6076,
    "plugins": [],
    "appType": "rekit-cra",
    "folders": [
      "node",
      "public",
      "config",
      "scripts"
    ],
    "files": [
      "package.json"
    ],
    "css": "less"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "description": "rekit-app2 created by Rekit.",
  "devDependencies": {
    "antd": "^3.10.7",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "connected-react-router": "^5.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "electron": "^3.0.8",
    "electron-builder": "^20.38.2",
    "electron-rebuild": "^1.8.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express-history-api-fallback": "^2.2.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nock": "^9.2.3",
    "node-pty": "^0.7.8",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  }
}
